{% block content %}
    <div class="card mb-4 shadow">
        <div class="card-body">
            <h5>{{ rounds.date }}</h5>
            <h6>{{ rounds.score }}</h6>
            <p>{{ rounds.details }}</p>
            <div class="d-flex justify-content-between">
                {% if rounds.owner == userId %}
<form action="/rounds/delete/{{ rounds.id }}/{{ rounds._id }}?_method=DELETE" method="POST">
    <input type="submit" value = "X">
</form>
{% endif %}
            </div>
        </div>
    </div>
{% endblock %}